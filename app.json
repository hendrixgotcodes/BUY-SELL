{
  "expo": {
    "name": "donewithit",
    "slug": "donewithit",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/logo-red.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      // "backgroundColor": "#e63e4b",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
